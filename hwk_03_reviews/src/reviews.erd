// Description: An ERD for a Paper Reviewing System

erdiagram ConferenceReviews
notation = crowsfoot

entity Papers { 
    id key
    title
    abstract
    efile_name
}

entity Authors { 
    email key
    first_name
    last_name
}

entity Reviewers { 
    Reviewer_email key
    first_name
    last_name
    phone_number
    affiliation
    interest_topic
}

